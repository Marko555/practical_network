public class UserLaptop {

  private String username;
  private String password;

  public UserLaptop(String name, String pass){
    this.username = name;
    this.password = pass;
  }
  
  
  public int compareTo(String name){
	  if (this.username.compareTo(name) == -1)
		  return -1;
	  else if (this.username.compareTo(name) == 1)
		  return 1;
	  else
		  return 0;
  }
  
  


	public String getUsername() {
		return username;
	}
	
	
	


	@Override
	public String toString() {
		return "UserLaptop [username=" + username + ", password=" + password
				+ ", getUsername()=" + getUsername() + ", getPassword()="
				+ getPassword() + "]";
	}


	public void setUsername(String username) {
		this.username = username;
	}
	
	
	public String getPassword() {
		return password;
	}
	
	
	public void setPassword(String password) {
		this.password = password;
	}
  
}
